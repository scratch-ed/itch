{
  "name": "scratch4d-judge",
  "version": "0.0.1",
  "description": "Scratch judge for Dodona",
  "contributors": [
    "Nils Mak",
    "Niko Strijbol",
    "Cedric Vanhaverbeke"
  ],
  "license": "MIT",
  "main": "index.js",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "esdoc": "^1.1.0",
    "esdoc-standard-plugin": "^1.0.0",
    "jszip": "^3.2.1",
    "lodash": "^4.17.20",
    "puppeteer": "^5.5.0"
  },
  "devDependencies": {
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1"
  },
  "scripts": {
    "start": "npm --prefix ./core run build && node fileEval.js",
    "clean": "rm -rf ./node_modules && rm -rf ./core/node_modules",
    "preinstall": "npm --prefix ./core install",
    "test": "jest"
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSpacing": true,
    "embeddedLanguageFormatting": "auto",
    "htmlWhitespaceSensitivity": "css",
    "insertPragma": false,
    "jsxBracketSameLine": false,
    "jsxSingleQuote": false,
    "printWidth": 80,
    "proseWrap": "preserve",
    "quoteProps": "as-needed",
    "requirePragma": false,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all",
    "useTabs": false
  },
  "workspaces": [
    "core"
  ]
}

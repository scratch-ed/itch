{
  "name": "scratch4d-judge",
  "version": "0.0.1",
  "description": "Scratch judge for Dodona",
  "contributors": [
    "Nils Mak",
    "Niko Strijbol",
    "Cedric Vanhaverbeke"
  ],
  "license": "MIT",
  "main": "index.js",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "esdoc": "^1.1.0",
    "esdoc-standard-plugin": "^1.0.0",
    "itch-judge": "file:./core",
    "jszip": "^3.2.1",
    "lodash": "^4.17.21",
    "puppeteer": "^8.0.0",
    "scratch-audio": "0.1.0-prerelease.20200528195344",
    "scratch-render": "0.1.0-prerelease.20201113223804",
    "scratch-storage": "^1.3.3",
    "scratch-svg-renderer": "^0.2.0-prerelease.20210225205629",
    "scratch-vm": "^0.2.0-prerelease.20210312110638"
  },
  "devDependencies": {
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "webpack-bundle-analyzer": "^4.4.0"
  },
  "scripts": {
    "start": "npm --prefix ./core run build && node fileEval.js",
    "api": "npm --prefix ./api start",
    "install:core": "cd ./core && npm install && cd ..",
    "install:api": "cd ./api && npm install && cd ..",
    "preinstall": "npm run install:core && npm run install:api",
    "clean": "rm -rf ./node_modules && rm -rf ./core/node_modules && rm -rf ./api/node_modules",
    "build": "npm --prefix ./core run build",
    "test": "jest tests"
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSpacing": true,
    "embeddedLanguageFormatting": "auto",
    "htmlWhitespaceSensitivity": "css",
    "insertPragma": false,
    "jsxBracketSameLine": false,
    "jsxSingleQuote": false,
    "printWidth": 80,
    "proseWrap": "preserve",
    "quoteProps": "as-needed",
    "requirePragma": false,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all",
    "useTabs": false
  }
}
